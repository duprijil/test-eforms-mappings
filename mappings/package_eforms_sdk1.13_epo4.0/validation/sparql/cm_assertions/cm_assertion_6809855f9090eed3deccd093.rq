#title: BT-300-Procedure-Language
#description: The corresponding XML element is /*/cac:ProcurementProject/cbc:Note/@languageID. The expected ontology instances are epo: epo:Procedure / rdf:langString .
#xpath: /*/cac:ProcurementProject/cbc:Note/@languageID

PREFIX epo: <http://data.europa.eu/a4g/ontology#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

ASK WHERE { 
	?this rdf:type epo:Procedure .
	?this epo:hasAdditionalInformation ?value .   FILTER(datatype(?value) = rdf:langString) 
}